	public static void main(String[] args) {
		
	}

}

class MyMath{
	long add(long am long b) {
		long result = a + b;
		return result;
	}
	long subtract(long a, long b) {
		return a - b;
	}
	long multiply(long a, long b) {
		return a * b;
	}
	long divide(double a, double b) {
		return a / b;
	}
}
